<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" style="-fx-border-style: solid;" xmlns="http://javafx.com/javafx/8.0.102-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cydercode.cams.NodeController">
   <children>
      <HBox prefHeight="0.0" prefWidth="400.0" spacing="5.0">
         <children>
            <Label text="URL" />
            <TextField fx:id="urlField" HBox.hgrow="ALWAYS" />
            <Button fx:id="removeButton" mnemonicParsing="false" text="Remove" />
         </children>
      </HBox>
      <VBox prefHeight="0.0" prefWidth="100.0">
         <children>
            <Label text="Instance name" />
            <Label fx:id="instanceNameLabel" text="Tigaro" />
         </children>
      </VBox>
      <VBox prefHeight="0.0" prefWidth="100.0">
         <children>
            <Label text="Health" />
            <Label fx:id="healthLabel" text="Healhy" />
         </children>
      </VBox>
      <VBox prefHeight="0.0" prefWidth="100.0">
         <children>
            <Label text="App uptime" />
            <Label fx:id="appUptimeLabel" text="123" />
         </children>
      </VBox>
      <VBox prefHeight="0.0" prefWidth="100.0">
         <children>
            <Label text="System uptime" />
            <Label fx:id="systemUptimeLabel" text="143245" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
